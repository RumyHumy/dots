mkdir -p ~/Torrent
transmission-daemon --download-dir ~/Torrent
transmission-remote -a <file>
transmission-remote -l

# Void
~/.config/transmission-daemon/settings.json

# SYSTEMD
sudo systecmtl stop transmission-daemon
